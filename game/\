#!/usr/bin/env python3
#
message = 'This Seattle landmark is '
print('How many stories is the Space needle?')
vconnection = float(input())
if vconnection == 6:
	message = message + '6 stories high!'
elif vconnection >= 7:
	message = message + 'less than that.'
elif vconnection >= 0:
	message = message + 'more than that.'
else:
	message = 'Try again.'
print(message)
